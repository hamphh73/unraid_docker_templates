<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Homegear8</Name>
  <Overview>Homegear8</Overview>
  <Description>Homegear8</Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>homegear/homegear:nightly</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2001</HostPort>
        <ContainerPort>2001</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2002</HostPort>
        <ContainerPort>2002</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2003</HostPort>
        <ContainerPort>2003</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/homegear8/etc</HostDir>
      <ContainerDir>/etc/homegear</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/homegear8/lib</HostDir>
      <ContainerDir>/var/lib/homegear</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_logs/homegear8</HostDir>
      <ContainerDir>/var/log/homegear</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>https://i.imgur.com/VhtqKLa.png</Icon>
  <Beta>False</Beta>
  <Category>HomeAutomation:</Category>
  <Config Name="HOST_USER_ID" Target="HOST_USER_ID" Default="99" Mode="" Description="User: nobody" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="HOST_USER_GID" Target="HOST_USER_GID" Default="100" Mode="" Description="Group: users" Type="Variable" Display="always" Required="false" Mask="false"/>
</Container>