<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Serviio</Name>
  <Overview>Serviio DNLA-Server</Overview>
  <Description>
  </Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>hamphh/serviio</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1900</HostPort>
        <ContainerPort>1900</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>8895</HostPort>
        <ContainerPort>8895</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>23423</HostPort>
        <ContainerPort>23423</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>23424</HostPort>
        <ContainerPort>23424</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/serviio/</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Musik/</HostDir>
      <ContainerDir>/media</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_logs/</HostDir>
      <ContainerDir>/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI>http://[IP]:[PORT:23423]/console</WebUI>
  <Banner></Banner>
  <Icon>https://i.imgur.com/kFWbuMA.png</Icon>
  <Beta>False</Beta>
  <Category>MediaServer:Music</Category>
</Container>