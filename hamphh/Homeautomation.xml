<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Homeautomation</Name>
  <Overview>Homegear and openHab.</Overview>
  <Description>openHab on port 8080</Description>
  <Support></Support>
  <Registry>https://hub.docker.com/r/hamphh/homeautomation/</Registry>
  <Repository>hamphh/homeautomation</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2001</HostPort>
        <ContainerPort>2001</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2002</HostPort>
        <ContainerPort>2002</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2003</HostPort>
        <ContainerPort>2003</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/homeautomation/</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_logs/</HostDir>
      <ContainerDir>/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/4YjuESp.png</Icon>
  <Beta>False</Beta>
  <Category>HomeAutomation:</Category>
  <ExtraParams></ExtraParams>
</Container>