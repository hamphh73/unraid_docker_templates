<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Graylog</Name>
  <Overview>Graylog all in one</Overview>
  <Description>
	Graylog with mongodb and elasticsearch[br][br]&#13;
	set ENV REST_TRANSPORT_URI with http://&lt;Host-IP&gt;:12900/
  </Description>
  <Support></Support>
  <Registry>/</Registry>
  <Repository>hamphh/graylog</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
	<Variable>
      <Name>REST_TRANSPORT_URI</Name>
      <Value>http://&lt;Host-IP&gt;:12900/</Value>
    </Variable>
	<Variable>
      <Name>LOGSTASH_TRACE</Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>12901</HostPort>
        <ContainerPort>9000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>12900</HostPort>
        <ContainerPort>12900</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>12902</HostPort>
        <ContainerPort>12902</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>12903</HostPort>
        <ContainerPort>12903</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>12906</HostPort>
        <ContainerPort>12906</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>12907</HostPort>
        <ContainerPort>12907</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/graylog/</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_logs/</HostDir>
      <ContainerDir>/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/ddnEfwW.png</Icon>
  <Beta>False</Beta>
  <Category>Tools:System</Category>
  <ExtraParams></ExtraParams>
</Container>