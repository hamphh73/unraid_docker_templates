<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Autoheal</Name>
  <Overview>Docker autoheal</Overview>
  <Description>
		Restart unhealthy container
  </Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>willfarrell/autoheal</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>none</Mode>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/var/run/docker.sock</HostDir>
      <ContainerDir>/var/run/docker.sock</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>https://i.imgur.com/mYTIRq1.png</Icon>
  <Beta>False</Beta>
  <Category>Tools:System</Category>
  <Config Name="AUTOHEAL_CONTAINER_LABEL" Target="AUTOHEAL_CONTAINER_LABEL" Default="all" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="AUTOHEAL_INTERVAL" Target="AUTOHEAL_INTERVAL" Default="30" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="true"/>
  <Config Name="AUTOHEAL_DEFAULT_STOP_TIMEOUT" Target="AUTOHEAL_DEFAULT_STOP_TIMEOUT" Default="30" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="true"/>
</Container>