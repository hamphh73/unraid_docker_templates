<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Nextcloud</Name>
  <Overview>Nextcloud with apache2/php7/mariadb</Overview>
  <Description>
		[b]nextcloud database[/b] : nextcloud[br][br]&#13;
		[b]nextcloud database user[/b] : nextclouduser[br][br]&#13;
		[b]nextcloud data dir[/b] : /data/nextcloud [br][br]&#13;
		[b]Fill in passwords in environment![/b]
  </Description>
  <Support></Support>
  <Registry>https://hub.docker.com/r/hamphh/nextcloud/</Registry>
  <Repository>hamphh/nextcloud</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Value></Value>
      <Name>mariadb_root_password</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>nextcloud_db_password</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8888</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/nextcloud/</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_logs/</HostDir>
      <ContainerDir>/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI>http://[IP]:[PORT:8888]</WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/PBW0Ssu.png</Icon>
  <Beta>False</Beta>
  <Category>Cloud:</Category>
</Container>