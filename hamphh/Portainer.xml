<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Portainer</Name>
  <Overview>Docker Management UI</Overview>
  <Description>
		Docker Management UI
  </Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>portainer/portainer</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9001</HostPort>
        <ContainerPort>9000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/var/run/docker.sock</HostDir>
      <ContainerDir>/var/run/docker.sock</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI>http://[IP]:[PORT:9001]</WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/nNQ32rT.png</Icon>
  <Beta>False</Beta>
  <Category>Tools:System</Category>
</Container>