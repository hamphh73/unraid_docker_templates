<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Download</Name>
  <Overview>deluge, Couchpotato and pyload</Overview>
  <Description>Port 8112: deluge[br]Port 5050: Couchpotato[br]Port 8000: pyload[br][br]User/Password always 'download'</Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>hamphh/download</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8112</HostPort>
        <ContainerPort>8112</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5050</HostPort>
        <ContainerPort>5050</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8000</HostPort>
        <ContainerPort>8000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9666</HostPort>
        <ContainerPort>9666</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/download/</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_logs/</HostDir>
      <ContainerDir>/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Dokumente/Temp/</HostDir>
      <ContainerDir>/downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/9mwihwU.jpg</Icon>
  <Beta>False</Beta>
  <Category>Downloaders:</Category>
  <ExtraParams></ExtraParams>
</Container>