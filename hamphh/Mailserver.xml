<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Mailserver</Name>
  <Overview>Mailserver mit postfix und dovecot</Overview>
  <Description>
  User/Mailadress: notify@kellerserver.home
  </Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>hamphh/mailserver</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>25</HostPort>
        <ContainerPort>25</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>993</HostPort>
        <ContainerPort>993</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/mailserver/mailboxes</HostDir>
      <ContainerDir>/var/vmail</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/mailserver/scripts</HostDir>
      <ContainerDir>/opt/scripts</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/mailserver/ssl</HostDir>
      <ContainerDir>/etc/dovecot/private</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_logs/</HostDir>
      <ContainerDir>/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI>http://[IP]:[PORT:xxx]</WebUI>
  <Banner></Banner>
  <Icon>https://i.imgur.com/CQqspX7.png</Icon>
  <Beta>False</Beta>
  <Category></Category>
  <ExtraParams></ExtraParams>
</Container>