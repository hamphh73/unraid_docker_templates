<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Elk</Name>
  <Overview></Overview>
  <Description></Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>sebp/elk:latest</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5044</HostPort>
        <ContainerPort>5044</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5601</HostPort>
        <ContainerPort>5601</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9200</HostPort>
        <ContainerPort>9200</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/elk/elastic</HostDir>
      <ContainerDir>/var/lib/elasticsearch</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI>http://[IP]:[PORT:5601]</WebUI>
  <Banner></Banner>
  <Icon>https://i.imgur.com/ZIrtdbK.png</Icon>
  <Beta>False</Beta>
</Container>