<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Qbittorrent</Name>
  <Overview></Overview>
  <Description>
  </Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>linuxserver/qbittorrent</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>6881</HostPort>
        <ContainerPort>6881</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>6881</HostPort>
        <ContainerPort>6881</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>8082</HostPort>
        <ContainerPort>8082</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/qbittorrent/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Share/</HostDir>
      <ContainerDir>/downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI>http://[IP]:[PORT:8082]</WebUI>
  <Banner></Banner>
  <Icon>https://i.imgur.com/vkPPL0n.png</Icon>
  <Beta>False</Beta>
  <Category></Category>
  <ExtraParams></ExtraParams>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="User: nobody" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Group: users" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="WEBUI_PORT" Target="WEBUI_PORT" Default="8082" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false"/>
</Container>