<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Homegear</Name>
  <Overview>Homegear</Overview>
  <Description>Homegear</Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>homegear/homegear:stable</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2001</HostPort>
        <ContainerPort>2001</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2002</HostPort>
        <ContainerPort>2002</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>2003</HostPort>
        <ContainerPort>2003</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/homegear/etc</HostDir>
      <ContainerDir>/etc/homegear</ContainerDir>
      <Mode>Z</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/homegear/lib</HostDir>
      <ContainerDir>/var/lib/homegear</ContainerDir>
      <Mode>Z</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_logs/homegear</HostDir>
      <ContainerDir>/var/log/homegear</ContainerDir>
      <Mode>Z</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/4YjuESp.png</Icon>
  <Beta>False</Beta>
  <Category>HomeAutomation:</Category>
  <ExtraParams>-e HOST_USER_ID=$(id -u) -e HOST_USER_GID=$(id -g)</ExtraParams>
</Container>