<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>openHAB</Name>
  <Overview>openHAB</Overview>
  <Description>openHAB on port 8080</Description>
  <Support></Support>
  <Registry></Registry>
  <Repository>openhab/openhab:2.3.0-amd64-debian</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>5007</HostPort>
        <ContainerPort>5007</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/openhab/addons</HostDir>
      <ContainerDir>/opt/openhab/addons</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/openhab/conf</HostDir>
      <ContainerDir>/opt/openhab/conf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/_dockerdata/openhab/userdata</HostDir>
      <ContainerDir>/opt/openhab/userdata</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>0.1</Version>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/4YjuESp.png</Icon>
  <Beta>False</Beta>
  <Category>HomeAutomation:</Category>
  <Config Name="USER_ID" Target="USER_ID" Default="99" Mode="" Description="User: nobody" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="GROUP_ID" Target="GROUP_ID" Default="100" Mode="" Description="Group: users" Type="Variable" Display="always" Required="false" Mask="false"/>
</Container>